package com.collection;

public class CreditCard2 {

	private String name;
	private int number;
	private int expire_year;

	public CreditCard2(String name, int number, int expire_year) {
		this.name = name;
		this.number = number;
		this.expire_year = expire_year;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public int getExpire_year() {
		return expire_year;
	}

	public void setExpire_year(int expire_year) {
		this.expire_year = expire_year;
	}

	@Override
	public String toString() {
		return "CreditCard [name=" + name + ", number=" + number + ", expire_year=" + expire_year + "]";
	}

}