public class GradeCalculatorCollection {
	
	public static void main(String[] args) {
		ArrayList<String> name = new ArrayList<>();
		ArrayList<Integer> rollno = new ArrayList<>();
		ArrayList<Double> marks = new ArrayList<>();
		
		Scanner sc = new Scanner(System.in);
		
		
		if(marks.equals(90)  && marks.equals(100)) {
			System.out.println("Student "+name+" scored "+ "Grade-A");
		}else if(marks.equals(80) && marks.equals(90)) {
			System.out.println("Student "+name+" scored "+ "Grade-B");
		}else if(marks.equals(70) && marks.equals(80)) {
			System.out.println("Student " +name+" scored "+ "Grade-C");
		}else {
			System.out.println("Student "+name+" scored "+ "Failed!");
		}
		
		
		
		
	}

}